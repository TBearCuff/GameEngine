####### Compile

GameEngineApp.o: ../../Source/GameEngine/GameEngineApp.cpp ../../Source/GameEngine/GameEngineApp.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o GameEngineApp.o ../../Source/GameEngine/GameEngineApp.cpp

####### Install

install_target: first FORCE
	@test -d $(INSTALL_ROOT)/usr/lib || mkdir -p $(INSTALL_ROOT)/usr/lib
	-$(INSTALL_FILE) ../../Lib/$(TARGET) $(INSTALL_ROOT)/usr/lib/$(TARGET)

uninstall_target: FORCE
	-$(DEL_FILE) $(INSTALL_ROOT)/usr/lib/$(TARGET)
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/lib/ 


install: install_target  FORCE

uninstall: uninstall_target  FORCE

FORCE:

